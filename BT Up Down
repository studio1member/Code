using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.EventSystems; //ThÃªm

public class ButtonMove : MonoBehaviour, IPointerUpHandler, IPointerDownHandler
{
    public void OnPointerDown(PointerEventData data)
    {
        if (gameObject.name == "MoveLeft Button") Debug.Log("Down");
    }
    public void OnPointerUp(PointerEventData data)
    {
        Debug.Log("Up");
    }
}
